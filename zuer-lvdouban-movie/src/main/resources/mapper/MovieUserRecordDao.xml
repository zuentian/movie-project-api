<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zuer.zuerlvdoubanmovie.dao.MovieUserRecordDao">

    <insert id="replaceIntoMovieUserRecord">
        Replace into movie_user_record
        (MOVIE_ID,USER_ID,STATE,SCORE,CRT_TIME,SHORT_COMMAND)
        VALUES
        (#{movieId},#{userId},#{state},#{score},NOW(),#{shortCommand})
    </insert>

</mapper>