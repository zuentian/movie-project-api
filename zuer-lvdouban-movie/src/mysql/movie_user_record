create table if not exists movie_user_record
(
	ID int auto_increment
		primary key,
	MOVIE_ID varchar(36) null,
	USER_ID varchar(36) null,
	STATE varchar(2) null comment '1-想看 2-看过',
	SCORE decimal(2,1) default 0.0 null comment '评分',
	CRT_TIME datetime default CURRENT_TIMESTAMP null
)
comment '用户和电影关联记录表';