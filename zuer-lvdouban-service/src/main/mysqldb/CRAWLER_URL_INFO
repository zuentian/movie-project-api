CREATE TABLE CRAWLER_URL_INFO
(
    id varchar(36) NOT NULL,
    UPD_TIME DATE,
    CRT_TIME DATE,
    URL varchar(500),
    URL_NAME varchar(200),
    TYPE varchar(100),
    TYPE_NAME varchar(100),
    PRIMARY KEY ( ID )
)ENGINE=InnoDB DEFAULT CHARSET=UTF8MB4;
alter table CRAWLER_URL_INFO comment '爬虫地址字典表';

ALTER TABLE CRAWLER_URL_INFO ADD UPD_USER VARCHAR(50) NULL;
ALTER TABLE CRAWLER_URL_INFO ADD CRT_USER VARCHAR(50) NULL;
ALTER TABLE CRAWLER_URL_INFO ADD UPD_NAME VARCHAR(50) NULL;
ALTER TABLE CRAWLER_URL_INFO ADD CRT_NAME VARCHAR(50) NULL;